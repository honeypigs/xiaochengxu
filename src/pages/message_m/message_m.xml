<scroll-view scroll-y="true" class="message_m">
	<view class="joinGroup">
		{{groupName}}
	</view>
	<view class="group-list" id="{{item.id}}" wx:for="{{groupUser}}">
		<image src="../../images/未知状态.png"></image>
		<text class="groupName">{{item.userName}}</text>
		<text class="phone">{{item.phone}}</text>
	</view>
</scroll-view>
